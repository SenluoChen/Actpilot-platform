{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "composite": true
  },
  "include": [
    "bin",
    "lib",
    "lambda",
    "scripts"
  ]
}
